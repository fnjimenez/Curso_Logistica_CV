   "source": [
    "def visualizar_red_hub_ciudad(resultado, filename='red_buenfin2025.png'):\n",
    "    asignaciones = resultado['asignaciones']\n",
    "    oferta = resultado['oferta']\n",
    "    demanda = resultado['demanda']\n",
    "\n",
    "    hubs = list(oferta.keys())\n",
    "    destinos = list(demanda.keys())\n",
    "\n",
    "    G = nx.DiGraph()\n",
    "    pos = {}\n",
    "\n",
    "    # HUBs a la izquierda (x=0)\n",
    "    for i, h in enumerate(hubs):\n",
    "        y = 1 - (i + 1)/(len(hubs)+1)\n",
    "        pos[h] = (0, y)\n",
    "        G.add_node(h, tipo='hub')\n",
    "\n",
    "    # Destinos a la derecha (x=1)\n",
    "    for j, d in enumerate(destinos):\n",
    "        y = 1 - (j + 1)/(len(destinos)+1)\n",
    "        pos[d] = (1, y)\n",
    "        G.add_node(d, tipo='dest')\n",
    "\n",
    "    # Arcos seg√∫n asignaciones\n",
    "    if len(asignaciones) == 0:\n",
    "        print(\"(Advertencia) No hay asignaciones positivas para graficar\")\n",
    "\n",
    "    max_flujo = max(asignaciones.values()) if len(asignaciones)>0 else 1\n",
    "\n",
    "    for (h, d), flujo in asignaciones.items():\n",
    "        ancho = 1 + 5*(flujo/max_flujo)\n",
    "        G.add_edge(h, d, weight=flujo, ancho=ancho)\n",
    "\n",
    "    fig, ax = plt.subplots(figsize=(16,10))\n",
    "\n",
    "    hubs_nodes = [n for n in G.nodes() if G.nodes[n]['tipo']=='hub']\n",
    "    dest_nodes = [n for n in G.nodes() if G.nodes[n]['tipo']=='dest']\n",
    "\n",
    "    # HUBs (azul oscuro tipo Amazon tech)\n",
    "    nx.draw_networkx_nodes(G, pos, nodelist=hubs_nodes,\n",
    "                          node_color='#146EB4', node_size=3000,\n",
    "                          node_shape='s', edgecolors='black', linewidths=2,\n",
    "                          ax=ax)\n",
    "\n",
    "    # Destinos (naranja Amazon)\n",
    "    nx.draw_networkx_nodes(G, pos, nodelist=dest_nodes,\n",
    "                          node_color='#FF9900', node_size=2600,\n",
    "                          node_shape='o', edgecolors='black', linewidths=2,\n",
    "                          ax=ax)\n",
    "\n",
    "    # Edges\n",
    "    edges_list = list(G.edges())\n",
    "    widths = [G[u][v]['ancho'] for (u,v) in edges_list]\n",
    "    nx.draw_networkx_edges(G, pos, edgelist=edges_list,\n",
    "                          width=widths, alpha=0.7,\n",
    "                          arrows=True, arrowsize=25,\n",
    "                          edge_color='#232F3E',  # negro Amazon brand\n",
    "                          arrowstyle='->',\n",
    "                          connectionstyle='arc3,rad=0.05',\n",
    "                          ax=ax)\n",
    "\n",
    "    # Labels de nodos (incluye % uso del HUB)\n",
    "    labels = {}\n",
    "    for h in hubs_nodes:\n",
    "        uso = resultado['utilizacion_hub'][h]*100\n",
    "        labels[h] = f\"{h}\\nUso {uso:.0f}%\"\n",
    "    for d in dest_nodes:\n",
    "        labels[d] = f\"{d}\\nDem {demanda[d]}\"\n",
    "    nx.draw_networkx_labels(G, pos, labels, font_weight='bold', ax=ax)\n",
    "\n",
    "    # Labels de arcos\n",
