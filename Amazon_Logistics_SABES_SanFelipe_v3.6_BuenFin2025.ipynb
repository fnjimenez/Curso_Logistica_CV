def optimizar(oferta, demanda, costos, mod5, presupuesto_max, costo_fijo_trailer=COSTO_FIJO_TRAILER):
    """
    Resuelve el problema de optimizaci√≥n de transporte
    """
    print("   üöÄ Iniciando proceso de optimizaci√≥n...")
    
    # Preparar datos
    hubs = list(oferta.keys())
    ciudades = list(demanda.keys())
    
    # Crear problema
    prob = LpProblem("Amazon_Logistics_BuenFin2025", LpMinimize)
    
    # Crear rutas posibles
    rutas = [(h, c) for h in hubs for c in ciudades if h in costos and c in costos[h]]
    
    # Variables de decisi√≥n
    x = LpVariable.dicts("Flujo", rutas, lowBound=0, cat="Integer")
    u = LpVariable.dicts("RutaActiva", rutas, cat="Binary")
    
    # FUNCI√ìN OBJETIVO: Minimizar costo total
    costo_transporte = lpSum(costos[h][c] * x[(h, c)] for (h, c) in rutas)
    costo_fijo = lpSum(costo_fijo_trailer * u[(h, c)] for (h, c) in rutas)
    prob += costo_transporte + costo_fijo
    
    # RESTRICCIONES
    
    # 1. No exceder capacidad de HUBs
    for h in hubs:
        prob += lpSum(x[(h, c)] for c in ciudades if (h, c) in x) <= oferta[h], f"Capacidad_{h}"
    
    # 2. Satisfacer demanda de ciudades
    for c in ciudades:
        prob += lpSum(x[(h, c)] for h in hubs if (h, c) in x) >= demanda[c], f"Demanda_{c}"
    
    # 3. Relaci√≥n flujo-activaci√≥n de ruta (Big M)
    for (h, c) in rutas:
        M = max(oferta[h], demanda[c])
        prob += x[(h, c)] <= M * u[(h, c)], f"Activacion_{h}_{c}"
    
    # 4. Presupuesto m√°ximo
    prob += (costo_transporte + costo_fijo) <= presupuesto_max, "Presupuesto_Maximo"
    
    # 5. RESTRICCIONES ESPECIALES seg√∫n mod5 (flujo)
    if mod5 == 0 and 'Monterrey' in hubs:
        prob += lpSum(x[('Monterrey', c)] for c in ciudades if ('Monterrey', c) in x) <= oferta['Monterrey'] * 0.7
        print("   üéØ Restricci√≥n: Monterrey al 70% de capacidad")
    
    if mod5 == 1 and 'Queretaro' in hubs:
        # La reducci√≥n de costos se aplica en generar_escenario_alumno
        prob += lpSum(x[('Queretaro', c)] for c in ciudades if ('Queretaro', c) in x) >= 1
        print("   üéØ Restricci√≥n: Quer√©taro debe operar (flujo >= 1)")
    
    if mod5 == 3 and 'CDMX_Norte' in hubs and 'Cancun' in ciudades:
        if ('CDMX_Norte', 'Cancun') in x:
            prob += x[('CDMX_Norte', 'Cancun')] >= 0.3 * demanda['Cancun']
            print("   üéØ Restricci√≥n: CDMX debe surtir 30% de Canc√∫n")
    
    if mod5 == 4 and 'Tijuana' in hubs:
        rutas_tijuana = [('Tijuana', c) for c in ciudades if ('Tijuana', c) in u]
        if rutas_tijuana:
            prob += lpSum(u[r] for r in rutas_tijuana) <= 3
            print("   üéØ Restricci√≥n: Tijuana m√°ximo 3 rutas activas")
            
    # La restricci√≥n mod5=2 (Costo Puebla +25%) se aplica en generar_escenario_alumno

    # RESOLVER
    print("   üîç Ejecutando solver PuLP...")
    prob.solve(PULP_CBC_CMD(msg=0))
    
    # VERIFICAR SOLUCI√ìN
    if prob.status != 1:
        print(f"   ‚ùå ERROR: No se encontr√≥ soluci√≥n √≥ptima")
        print(f"   üí° Estado: {LpStatus[prob.status]}")
        print(f"   üí° Sugerencia: Aumenta presupuesto o reduce demanda")
        return None
    
    print(f"   ‚úÖ Soluci√≥n √≥ptima encontrada!")
    
    # RECOLECTAR RESULTADOS
    asignaciones = {}
    for (h, c) in rutas:
        valor = x[(h, c)].varValue
        if valor and valor > 0.1:
            asignaciones[(h, c)] = int(valor)
    
    # Calcular m√©tricas
    total_unidades = sum(asignaciones.values())
    rutas_activas = len(asignaciones)
    costo_total = value(prob.objective)
    
    # Calcular utilizaci√≥n
    utilizacion = {}
    for h in hubs:
        unidades_enviadas = sum(asignaciones.get((h, c), 0) for c in ciudades)
        utilizacion[h] = unidades_enviadas / oferta[h] if oferta[h] > 0 else 0
    
    return {
        'status': LpStatus[prob.status],
        'costo_total': costo_total,
        'asignaciones': asignaciones,
        'utilizacion': utilizacion,
        'oferta': oferta,
        'demanda': demanda,
        'presupuesto': presupuesto_max,
        'total_unidades': total_unidades,
        'rutas_activas': rutas_activas
    }

print("‚úÖ Solver de optimizaci√≥n listo")

